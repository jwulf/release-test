{
    "branches": ["main", {
        "name":  "develop",
        "prerelease": true
    }],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      {
        "path": "@semantic-release/npm",
        "npmPublish": false
      },
      {
        "path": "@semantic-release/github",
        "assets": "dist/**",
        "pkgRoot": "dist"
      }
    ]
  }